<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.careyq.explore.server.mapper.AttachmentMapper">

    <select id="selectExistByCategory" resultType="java.lang.Integer">
        select 1 from attachment where is_del = 0 and category_id = #{categoryId} limit 1;
    </select>
</mapper>
